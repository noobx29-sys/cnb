{
  "expo": {
    "name": "CNB Carpets",
    "slug": "cnb",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/cnb-app.png",
    "scheme": "cnb",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "developmentClient": false,
    "splash": {
      "image": "./assets/images/cnb-app.png",
      "resizeMode": "contain",
      "backgroundColor": "#FB8A13"
    },
    "ios": {
      "bundleIdentifier": "com.cnb.app",
      "buildNumber": "2",
      "versionCode": 1,
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to location to show the office locations on the map.",
        "NSLocationAlwaysUsageDescription": "This app needs access to location to show the office locations on the map."
      }
    },
    "android": {
      "package": "com.cnb.app",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/cnb-app.png",
        "backgroundColor": "#FB8A13"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/cnb-app.png",
          "imageWidth": 100,
          "resizeMode": "contain",
          "backgroundColor": "#FB8A13"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "151b57ce-bc3b-4490-8b1a-076e7828c2f2"
      },
      "firebase": {
        "apiKey": "AIzaSyB558CviT50ttHxIeSVROTEpxhu_h8Wias",
        "authDomain": "cnb-app-f2ed6.firebaseapp.com",
        "databaseURL": "https://cnb-app-f2ed6.firebaseio.com",
        "projectId": "cnb-app-f2ed6",
        "storageBucket": "cnb-app-f2ed6.firebasestorage.app",
        "messagingSenderId": "465463891466",
        "appId": "1:465463891466:ios:91389050378e1701b9f1f3"
      }
    }
  }
}
